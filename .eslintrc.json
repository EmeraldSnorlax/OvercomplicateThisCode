{
  "rules": {
    "for-direction": 1,
    "getter-return": 1,
    "no-async-promise-executor": 1,
    "no-await-in-loop":1,
    "no-compare-neg-zero":1,
    "no-cond-assign":1,
    "no-console":1,
    "no-constant-condition":1,
    "no-control-regex":1,
    "no-debugger":1,
    "no-dupe-args":1,
    "no-dupe-else-if":1,
    "no-dupe-keys":1,
    "no-duplicate-case":1,
    "no-empty":1,
    "no-empty-character-case":1,
    "no-ex-assign":1,
    "no-extra-boolean-cast":1,
    "no-extra-parens":1,
    "no-extra-semi":1,
    "no-func-assign":1,
    "no-import-assign":1,
    "no-inner-declarations":1,
    "no-invalid-regexp":1,
    "no-irregular-whitespace":1,
    "no-loss-of-precision":1,
    "no-obj-calls":1,
    "no-promise-executor-return":1,
    "no-prototype-builtins":1,
    "no-regex-spaces":1,
    "no-setter-return":1,
    "no-sparse-arrays":1,
    "no-template-curly-in-strings":1,
    "no-unexpected-multiline":1,
    "no-unreachable-loop":1,
    "no-unsafe-finally":1,
    "no-unsafe-negation":1,
    "no-unsafe-optional-chaining":1,
    "no-useless-backrefrence":1,
    "require-atomic-updates":1,
    "use-isnan":1,
    "valid-typeof":1
  },
  "parser": "babel-eslint"
}
